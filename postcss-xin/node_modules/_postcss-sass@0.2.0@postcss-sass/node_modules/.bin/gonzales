#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_gonzales-pe@4.2.3@gonzales-pe/bin/gonzales.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_gonzales-pe@4.2.3@gonzales-pe/bin/gonzales.js" "$@"
  ret=$?
fi
exit $ret
