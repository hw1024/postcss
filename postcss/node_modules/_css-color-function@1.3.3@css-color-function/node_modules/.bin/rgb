#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_rgb@0.1.0@rgb/bin/rgb" "$@"
  ret=$?
else 
  node  "$basedir/../../../_rgb@0.1.0@rgb/bin/rgb" "$@"
  ret=$?
fi
exit $ret
